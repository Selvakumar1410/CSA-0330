#include <stdio.h>
#define SIZE 10

int hashTable[SIZE];

void insert(int key) {
    int index = key % SIZE;
    while (hashTable[index] != -1)
        index = (index + 1) % SIZE;
    hashTable[index] = key;
}

int main() {
    int i, n, key;

    for (i = 0; i < SIZE; i++)
        hashTable[i] = -1;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter key: ");
        scanf("%d", &key);
        insert(key);
    }

    printf("Hash Table:\n");
    for (i = 0; i < SIZE; i++)
        printf("%d -> %d\n", i, hashTable[i]);

    return 0;
}


Output:
Enter number of elements: 3
Enter key: 12
Enter key: 22
Enter key: 32
Hash Table:
0 -> -1
1 -> -1
2 -> 12
3 -> 22
4 -> 32
